{
  "commit_type": {
    "append_emoji_to_label": true,
    "append_emoji_to_commit": false
  },
  "commit_scope": {
    "enable": true,
    "initial_value": "main",
    "options": [
      {
        "value": "main",
        "label": "main"
      },
      {
        "value": "branch",
        "label": "branch"
      },
      {
        "value": "init",
        "label": "init"
      },
      {
        "value": "util",
        "label": "util"
      },
      {
        "value": "build",
        "label": "build"
      },
      {
        "value": "",
        "label": "none"
      }
    ]
  },
  "check_ticket": {
    "append_hashtag": true
  },
  "branch_user":{
    "enable": false
  },
  "branch_type":{
    "enable": true,
    "initial_value": "CCIFACOMM",
    "separator": "-",
    "options": [
      {
        "value": "CCIFACOMM",
        "label": "CCIFACOMM"
      },
      {
        "value": "",
        "label": "none"
      }
    ]
  },
  "enable_worktrees": false,
  "branch_ticket": {
    "enabled": true,
    "required": false,
    "separator": "/"
  },
  "branch_pre_commands": [
    "git stash",
    "git checkout main",
    "git pull -r origin main",
    "npm install"
  ]
}